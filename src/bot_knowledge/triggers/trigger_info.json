                    },
                    "start": {
                        "title": "Queue start",
                        "example": "megadork start jukebox"
                    },
                    "play_next": {
                        "title": "Queue play next",
                        "example": "megadork play next"
                    },
                    "next": {
                        "title": "Queue check next",
                        "example": "megadork jukebox next up"
