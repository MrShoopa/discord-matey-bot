                    "title": "Stop Music",
                    "example": "stop"
                },
                "args": {
                    "loop": {
                        "title": "Loop",
                        "example": "*request* looped"
                    },
                    "yt": {
                        "title": "YouTube search keyword",
                        "example": "youtube *searchName*"
                    },
                    "sc": {
                        "title": "SoundCloud search keyword",
                        "example": "soundcloud *searchName*"
                    }
